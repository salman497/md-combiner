{
    "name": "md-combiner",
    "version": "1.0.0",
    "description": "Combine markdown files recursively",
    "scripts": {
      "build": "tsc",
      "create-md-structure": "ts-node src/createStructure.ts",
      "use-structure": "ts-node src/useStructure.ts",
      "----------Project----------": "",
      "create-structure-prompt-foo": "ts-node src/createStructure.ts /Users/salmanaziz/git-code/promptfoo/site/docs",
      "create-structure-prompt-foo-example": "ts-node src/createStructure.ts /Users/salmanaziz/git-code/promptfoo/examples -includeOtherExtensions",
      "use-structure-prompt-foo": "node --max-old-space-size=8192 -r ts-node/register src/useStructure.ts ./result/git_code_promptfoo_site.json",
      "use-structure-prompt-foo-examples": "node --max-old-space-size=8192 -r ts-node/register src/useStructure.ts ./result/git_code_promptfoo_examples.json"


    },
    "dependencies": {
      "glob": "^10.3.10",
      "markdown-toc": "^1.2.0"
    },
    "devDependencies": {
      "@types/node": "^20.11.0",
      "ts-node": "^10.9.2",
      "typescript": "^5.3.3"
    }
  }